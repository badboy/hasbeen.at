<div class="attendees">

  <ul>
    <% @attendees.each do |attendee| %>
      <li class='attendee'>
      <p class="bracket">[</p>
        <div class='attendee-picture'>
          <img src='<%= attendee.avatar %>' />
        </div>
        <div class='attendee-name'>
          <%= link_to attendee.name, controller: 'attendees', action: 'show', id: attendee.name %>
        </div>
        <p class="bracket">]</p>
      </li>
    <% end %>
  </ul>
</div>
